<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
    <rule>
        <from>^/news/(.*)/(.*)$</from>
        <to>/rest/news/details/$1?title=$2</to>
    </rule>
    <rule>
        <from>^/products/(.*)$</from>
        <to>/product-details/$1</to>
    </rule>
</urlrewrite>